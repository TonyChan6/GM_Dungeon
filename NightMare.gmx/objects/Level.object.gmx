            <string>/// Level Generator

//create the grid for the level
var height = 25;
var width = 25;

room_width = width*2*CELL_WIDTH;
room_height = height*2*CELL_HEIGHT;

var mpGrid = ds_grid_create(width,height);
var tunnelGrid = mp_grid_create(0, 0, width, height, CELL_WIDTH, CELL_HEIGHT);

//fill the grid with VOID
ds_grid_set_region(mpGrid, 0, 0, width-1, height-1, VOID);

//the num of room *
var roomCount = 7;

var count = 0;

//Create the Random Room
repeat(100){

//random the size of room
    var roomsizew = 4 + irandom_range(0,2);
    var roomsizeh = 4 + irandom_range(0,2);

    var rx = irandom_range(0,width-roomsizew);
    var ry = irandom_range(0,height-roomsizeh); 
   
    if(ds_grid_get_sum(mpGrid, rx, ry, rx+roomsizew, ry+roomsizeh)==0 &amp;&amp; count &lt; roomCount){
        var roomGrid = ds_grid_create(roomsizew, roomsizeh);
        ds_grid_set_region(roomGrid, 0, 0, roomsizew-1, roomsizeh-1, FLOOR);
        ds_grid_add_grid_region(mpGrid, roomGrid, 0, 0, roomsizew, roomsizeh, rx, ry);
        
        //Add room to the map
        
        count ++;
    }
}

//Draw the level
for (var yy = 0; yy &lt; height; yy++){
    for(var xx = 0; xx &lt; width; xx++) {
        if(mpGrid[# xx,yy] == VOID)
            tile_add(bg_floor, 0, 0,CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT, 0);
        else if(mpGrid[# xx,yy] == FLOOR)
            tile_add(bg_wall, 0, 0, CELL_WIDTH, CELL_HEIGHT, xx*CELL_WIDTH, yy*CELL_HEIGHT, 0);
    }
} 


